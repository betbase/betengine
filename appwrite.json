{
    "projectId": "betengine-dev-fra",
    "projectName": "BetEngine - Development",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": false,
                "invites": false,
                "anonymous": false,
                "email-otp": true,
                "magic-url": false,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "betengine",
            "name": "betengine",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "games",
            "$permissions": [],
            "databaseId": "betengine",
            "name": "games",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 32,
                    "default": null
                },
                {
                    "key": "nameShortened",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 16,
                    "default": null
                },
                {
                    "key": "logoId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 16,
                    "default": null
                }
            ],
            "indexes": []
        },
        {
            "$id": "tournaments",
            "$permissions": [],
            "databaseId": "betengine",
            "name": "Tournaments",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 64,
                    "default": null
                },
                {
                    "key": "nameShortened",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 32,
                    "default": null
                },
                {
                    "key": "logoUrl",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 256,
                    "default": null
                },
                {
                    "key": "startDate",
                    "type": "datetime",
                    "required": false,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "endDate",
                    "type": "datetime",
                    "required": false,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "prizePool",
                    "type": "double",
                    "required": false,
                    "array": false,
                    "min": -1.7976931348623157e+308,
                    "max": 1.7976931348623157e+308,
                    "default": null
                },
                {
                    "key": "parentId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 16,
                    "default": null
                },
                {
                    "key": "children",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 16,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "parentId_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "parentId"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "teams",
            "$permissions": [],
            "databaseId": "betengine",
            "name": "Teams",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 32,
                    "default": null
                },
                {
                    "key": "colorPrimary",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 8,
                    "default": null
                },
                {
                    "key": "colorSecondary",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 8,
                    "default": null
