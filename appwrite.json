        {
            "$id": "series",
            "$permissions": [],
            "databaseId": "betengine",
            "name": "series",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [],
            "indexes": []
        }
    ],
    "functions": [
        {
            "$id": "get_scheduled_cs2_matches",
            "execute": [
                "any"
            ],
            "name": "Get Scheduled CS2 Matches",
            "enabled": true,
            "logging": true,
            "runtime": "bun-1.1",
            "scopes": [
                "users.read"
            ],
            "events": [],
            "schedule": "",
            "timeout": 15,
            "entrypoint": "src/main.ts",
            "commands": "bun install",
            "path": "functions/Get Scheduled CS2 Matches"
        }
    ]
